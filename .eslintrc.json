{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [2, "always"],
        "space-before-function-paren": [0, {"anonymous": "always", "named": "always"}],
        "camelcase": 0,
        "no-return-assign": 0
    },
    "settings": {
        "react": {
          "version": "detect"
        }
    }
}